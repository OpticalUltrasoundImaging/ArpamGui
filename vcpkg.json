{
  "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg-tool/main/docs/vcpkg.schema.json",
  "name": "arpam-gui",
  "version": "0.1",
  "dependencies": [
    "armadillo",
    "fftconv",
    "fmt",
    "gtest",
    "kfr",
    "qcustomplot",
    "qtbase",
    "rapidjson",
    "tbb",
    {
      "name": "opencv4",
      "features": ["tbb", "ipp", "tiff", "world"]
    },
    {
      "name": "fftw3",
      "platform": "x64",
      "features": ["avx2", "threads"]
    },
    {
      "name": "fftw3",
      "platform": "!x64",
      "features": ["threads"]
    }
  ]
}
