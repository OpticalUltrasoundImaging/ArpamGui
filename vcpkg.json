{
  "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg-tool/main/docs/vcpkg.schema.json",
  "name": "arpam-gui",
  "version": "0.1",
  "dependencies": [
    "qtbase",
    "rapidjson",
    "glslang",
    "glfw3",
    "armadillo",
    "fftconv",
    "openblas",
    {
      "name": "opencv4",
      "features": ["contrib", "dnn", "png", "vulkan", "tbb", "png", "world"]
    },
    {
      "name": "fftw3",
      "platform": "x64",
      "features": ["avx2", "threads"]
    },
    {
      "name": "fftw3",
      "platform": "!x64",
      "features": ["threads"]
    },
    {
      "name": "cuda",
      "platform": "x64"
    },
    {
      "name": "itk",
      "platform": "x64",
      "features": ["cuda", "cufftw", "opencv", "tbb"]
    },
    {
      "name": "itk",
      "platform": "!x64",
      "features": ["opencv", "tbb"]
    },
    "gtest",
    "cli11",
    "indicators"
  ],
  "builtin-baseline": "dee924de74e81388140a53c32a919ecec57d20ab"
