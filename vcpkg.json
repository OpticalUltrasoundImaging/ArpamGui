{
  "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg-tool/main/docs/vcpkg.schema.json",
  "name": "arpam-gui",
  "version": "0.1",
  "dependencies": [
    "fmt",
    "qtbase",
    "rapidjson",
    "glslang",
    "glfw3",
    "armadillo",
    "fftconv",
    "tbb",
    {
      "name": "opencv4",
      "features": [
        "contrib",
        "dnn",
        "png",
        "vulkan",
        "tbb",
        "png",
        "world"
      ]
    },
    {
      "name": "fftw3",
      "platform": "x64",
      "features": [
        "avx2",
        "threads"
      ]
    },
    {
      "name": "fftw3",
      "platform": "!x64",
      "features": [
        "threads"
      ]
    },
    "qcustomplot",
    "gtest",
    "cli11",
    "indicators"
  ]
}